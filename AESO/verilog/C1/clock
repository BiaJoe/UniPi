 .timescale 0 0;
v0x5ac558576730_0 .var "clock", 0 0;
    .scope S_0x5ac5585765a0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ac558576730_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5ac5585765a0;
T_1 ;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ac558576730_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ac558576730_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5ac5585765a0;
T_2 ;
    %vpi_call 2 15 "$monitor", "%t %d", $time, v0x5ac558576730_0 {0 0 0};
    %vpi_call 2 16 "$dumpfile", "clock.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, v0x5ac558576730_0 {0 0 0};
    %delay 30, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_2;
